    "@types/node": "^16.11.7",
    "autoprefixer": "^10.4.14",
    "daisyui": "^3.3.1",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "postcss": "^8.4.26",
